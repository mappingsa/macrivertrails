/* Load webfonts from the server (website) or local files (PhoneGap) */
/* Conversions done by fontsquirrel.com */

<%= partial "paths" %>

@font-face {
    font-family: 'Caviar Dreams';
    src: typeUrl('caviardreams-webfont.eot'); /* IE9 */
    src: typeUrl('caviardreams-webfont.eot?#iefix') format('embedded-opentype'), /* IE6-8 */
         typeUrl('caviardreams-webfont.woff') format('woff'),  /* Modern browsers */
         typeUrl('caviardreams-webfont.ttf') format('truetype'),  /* Safari, Android, iOS */
         typeUrl('caviardreams-webfont.svg#caviar_dreamsregular') format('svg'); /* legacy iOS */
    font-weight: normal;
    font-style: normal;

}

@font-face {
    font-family: 'Caviar Dreams';
    src: typeUrl('caviardreams-bold-webfont.eot');
    src: typeUrl('caviardreams-bold-webfont.eot?#iefix') format('embedded-opentype'),
         typeUrl('caviardreams-bold-webfont.woff') format('woff'),
         typeUrl('caviardreams-bold-webfont.ttf') format('truetype'),
         typeUrl('caviardreams-bold-webfont.svg#caviar_dreamsbold') format('svg');
    font-weight: bold;
    font-style: normal;

}
