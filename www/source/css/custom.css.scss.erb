  color:  #7cc4e7;
}

/* ThemeRoller can only set button border radius globally.
   Our theme sets button border radius
   But this affects other elements which
   should have a completely rounded appearance on the ends.
   Undo the global radius change for these elements */

/* Sliders and slide switches */
.ui-slider .ui-btn-corner-all,
.ui-slider .ui-slider-handle.ui-btn-corner-all,
.ui-slider.ui-btn-corner-all,
/* textless buttons (e.g. disclosure buttons) */
.ui-btn-icon-notext.ui-btn-corner-all
 {
  -moz-border-radius:    1em;
  -webkit-border-radius: 1em;
  border-radius:         1em;
}


/* =============================================================================
   Dialog (Modal Windows)
   ========================================================================== */

.ui-dialog {
  background-color: #222 !important;
  background-size: 20px 20px !important;
}

.ui-dialog .ui-dialog-contain {
  margin-top: 0 !important
}

/* =============================================================================
   Set your main background image & text styles
   ========================================================================== */
.ui-body-c,
.ui-dialog.ui-overlay-c {
   background-image: imageUrl('whitey.png');
   background-size: 203px 317px;
   text-shadow: none;
}


.map-page.ui-body-c,
.map-page .ui-body-c,
.browse-page.ui-body-c,
.browse-page .ui-body-c {
