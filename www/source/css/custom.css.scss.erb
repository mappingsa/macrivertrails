/* =============================================================================
   (ex) Fading place menu
   ========================================================================== */
.menu {
  background:#262C3A;
  -webkit-text-size-adjust:none !important;
  -moz-text-size-adjust:none !important;
  -o-text-size-adjust:none !important;
  -ms-text-size-adjust:none !important;
  text-size-adjust:none !important;
  position: absolute;
  right: 0;
  z-index: 1;
  visibility: hidden;
  opacity: 0;
  -webkit-transition: visibility 0s linear 0.6s, opacity 0.6s ease-out;
  -moz-transition: visibility 0s linear 0.6s, opacity 0.6s ease-out;
  -o-transition: visibility 0s linear 0.6s, opacity 0.6s ease-out;
  -ms-transition: visibility 0s linear 0.6s, opacity 0.6s ease-out;
  transition: visibility 0s linear 0.6s, opacity 0.6s ease-out;
}

.menu.slow {
  -webkit-transition: visibility 0s linear 1.2s, opacity 1.2s ease-out;
  -moz-transition: visibility 0s linear 1.2s, opacity 1.2s ease-out;
  -o-transition: visibility 0s linear 1.2s, opacity 1.2s ease-out;
  -ms-transition: visibility 0s linear 1.2s, opacity 1.2s ease-out;
  transition: visibility 0s linear 1.2s, opacity 1.2s ease-out;
}

.menu.visible {
  visibility: visible;
  opacity: 1.0;
  -webkit-transition-delay: 0s;
  -moz-transition-delay: 0s;
  -o-transition-delay: 0s;
  -ms-transition-delay: 0s;
  transition-delay: 0s;

}

.menu ul {
  margin: 0;
  padding: 0;
}

.menu img {
  float: left;
  margin-right: 10px;
}

.menu ul li {
  list-style-type: none;
  margin: 0 0;
}
.menu .ui-link, .menu .ui-link:visited {
  text-align: center;
