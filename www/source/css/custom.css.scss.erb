}


/* =============================================================================
   Body Links
   ========================================================================== */
.ui-body-c .ui-link-inherit {
  color:  #007acc;
}

.ui-body-c .ui-link {
  color:  #007acc;
  font-weight: normal;
}

.ui-body-c .ui-link:hover {
  color:  #007acc ;
}

.ui-body-c .ui-link:active {
  color:  #007acc ;
}

.ui-body-c .ui-link:visited {
    color:  #007acc;
}


/* =============================================================================
   Button Styles
   ========================================================================== */
.ui-btn-active a.ui-link-inherit {
  color:   #fff;
}

