.place-menu-icon {
  float: left;
  background-size: 100% auto;
}

.itin-header-title {
  text-align: center;
}

/* Todo page collapsible headers with split-button */
.todo-page .ui-collapsible-heading {
  margin-right: 40px;
  border-top-right-radius: 0;
  border-bottom-right-radius: 0;
}

.todo-page .ui-collapsible-heading .ui-btn {
  border-top-right-radius: 0;
  border-bottom-right-radius: 0;
}

.todo-collapsible {
  position: relative;
}

.todo-map-btn {
  border-top-right-radius:6px;
  border-bottom-right-radius:6px;
  display:block;
  position:absolute;
  top:0;
  right:0;
  width: 40px;
  height:40px;
  background:black;
}

.todo-map-btn-inner {
  background: transparent;
  color:white;
  background-image: imageUrl('103-map.png');
  background-repeat: none;
  position:absolute;
  top:6px;
  right:6px;
  display:block;
  width:26px;
  height:21px;
}






