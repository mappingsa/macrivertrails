}

/* Theme overrides. */
/* =============================================================================
   UI Bars (add to a div surrounding content, like a callout box)
   ========================================================================== */
.ui-bar-a,
.ui-bar-b,
.ui-bar-c {
  background-image: none;
}

.ui-bar-c .ui-link-inherit {
  color:    #3E3E3E;
}
