$buildTarget: '<%= build_target %>';
$baseURI: '<%= base_uri %>';
$imagePath: '<%= path_images %>';
$typePath: 'type/';

/*
   buildTarget: '#{$buildTarget}'
   baseURI: '#{$baseURI}'
   imagePath: '#{$imagePath}'
   typePath: '#{$typePath}'
*/

@function imageUrl($path) {
  @return url('#{$imagePath}#{$path}')
}

@function typeUrl($path) {
  @return url('#{$typePath}#{$path}')
}